server {
  listen              443 ssl;
  server_name         valid-keys-site2.com;
  ssl_certificate      test-files/valid-keys-site2.com/fullchain.pem;
  ssl_certificate_key  test-files/valid-keys-site2.com/privkey.pem;

  return 200 "using a secure nginx web server with Let's Encrypt installed"
  add_header Content-Type text/plain;
}
