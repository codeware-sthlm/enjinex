{
  "version": 2,
  "projects": {
    "init": {
      "root": "apps/init",
      "sourceRoot": "apps/init/src",
      "projectType": "application",
      "prefix": "init",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/init",
            "main": "apps/init/src/main.ts",
            "tsConfig": "apps/init/tsconfig.app.json",
            "assets": [
              "apps/init/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "sourceMap": false,
              "fileReplacements": [
                {
                  "replace": "apps/init/src/environments/environment.ts",
                  "with": "apps/init/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "init:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/init/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/init"
          ],
          "options": {
            "jestConfig": "apps/init/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "config": {
      "root": "libs/config",
      "sourceRoot": "libs/config/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/config/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/config"
          ],
          "options": {
            "jestConfig": "libs/config/jest.config.js",
